{
    "name": "cloudcreativity/laravel-json-api",
    "description": "JSON API (jsonapi.org) support for Laravel applications.",
    "keywords": [
        "jsonapi.org",
        "json-api",
        "jsonapi",
        "cloudcreativity",
        "json",
        "api",
        "laravel"
    ],
    "homepage": "https://github.com/cloudcreativity/laravel-json-api",
    "support": {
        "issues": "https://github.com/cloudcreativity/laravel-json-api/issues"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Cloud Creativity Ltd",
            "email": "info@cloudcreativity.co.uk"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "cloudcreativity/json-api": "^0.11",
        "illuminate/console": "5.4.*|5.5.*",
        "illuminate/contracts": "5.4.*|5.5.*",
        "illuminate/database": "5.4.*|5.5.*",
        "illuminate/filesystem": "5.4.*|5.5.*",
        "illuminate/http": "5.4.*|5.5.*",
        "illuminate/pagination": "5.4.*|5.5.*",
        "illuminate/support": "5.4.*|5.5.*",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "cloudcreativity/json-api-testing": "^0.1.1|^0.2",
        "orchestra/testbench": "^3.4",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "suggest": {
        "cloudcreativity/json-api-testing": "Required to use the test helpers."
    },
    "autoload": {
        "psr-4": {
            "CloudCreativity\\LaravelJsonApi\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CloudCreativity\\LaravelJsonApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.12.x-dev"
        },
        "laravel": {
            "providers": [
                "CloudCreativity\\LaravelJsonApi\\ServiceProvider"
            ],
            "aliases": {
                "JsonApi": "CloudCreativity\\LaravelJsonApi\\Facades\\JsonApi"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
